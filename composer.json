{
    "name": "famima65536/my-chunk-land",
    "description": "PocketMine-MP land protection and buy plugin for major Economy plugins.",
    "type": "project",
    "require-dev": {
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^9.5",
        "pocketmine/pocketmine-mp": "^4.0.0",
        "oirancage/miwallet": "^1.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "famima65536",
            "email": "famima65536@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "": "src"
        }
    },
    "require": {
        "ext-mysqli": "*",
        "ext-mbstring": "*"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan -c phpstan.neon"
    },
    "repositories": {
        "oirancage/miwallet": {
            "type": "vcs",
            "url": "https://github.com/OiranCage/MiWallet"
        }
    }
